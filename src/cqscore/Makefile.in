# Programs and options
# --------------------
@SET_MAKE@
@VARIABLES@
LIBS=@QT_LIBS@ $(X_LIBS) @LIBS@
LDFLAGS=@QT_LDFLAGS@ @LDFLAGS@ @EXE_LDFLAGS@ 
LIBDEP=
CXXFLAGS=@CXXFLAGS@ @QT_CXXFLAGS@
CPPFLAGS=@CPPFLAGS@ -I$(TOPDIR)/include/visit -I.

# Files
# -----
HDRMOC = CQScore.h

HDR    = $(HDRMOC)

SRC    = $(HDRMOC:.h=.C) \
         main.C 

# Derived objects
# ---------------
SRCMOC=$(HDRMOC:.h=_moc.C)
OBJMOC=$(SRCMOC:.C=.o)

OBJ=$(SRC:.C=.o) $(OBJMOC)


# Standard targets
# ----------------
all: cqscore

install: install_bin

clean:
	$(RM) $(OBJ) $(SRCMOC) cqscore

# Other targets
# -------------
cqscore: $(OBJ)
	$(CXX) -o $@ $(OBJ) $(LDFLAGS) $(LIBS)


moc: $(SRCMOC)

# Automatic dependency stuff
# --------------------------
@TARGETS@
