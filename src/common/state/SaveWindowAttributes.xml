<?xml version="1.0"?>
  <Attribute name="SaveWindowAttributes" purpose="This class contains the attributes used for saving windows." exportAPI="STATE_API" exportInclude="state_exports.h">
    <Enum name="FileFormat">
      BMP
      CURVE
      JPEG
      OBJ
      PNG
      POSTSCRIPT
      PPM
      RGB
      STL
      TIFF
      ULTRA
      VTK
    </Enum>
    <Field name="hostName" type="string">"localhost"</Field>
    <Field name="fileName" type="string">"visit"</Field>
    <Field name="family" type="bool">true</Field>
    <Field name="format" type="enum" subtype="FileFormat">TIFF</Field>
    <Field name="maintainAspect" type="bool">true</Field>
    <Field name="width" type="int">1024</Field>
    <Field name="height" type="int">1024</Field>
    <Field name="screenCapture" type="bool">true</Field>
    <Field name="saveTiled" type="bool">false</Field>
    <Field name="quality" type="int">80</Field>
    <Field name="progressive" type="bool">false</Field>
    <Field name="binary" type="bool">false</Field>
    <Field name="lastRealFilename" type="string" internal="true">""</Field>
  </Attribute>
