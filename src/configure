                VAC_CV_LIBDIR_DESTROY_LINKS="$VAC_CV_LIBDIR_DESTROY_LINKS; rm -f $fb"
            fi
            lib_links="$lib_links $f"
        done

        #
        # Handle dependent libs for this library
        #
        deplib_ldflags="`echo $withval | cut -d':' -f2 | tr ',' ' '`"
        deplib_dirs=$libdir
        deplib_libs=""
        deplib_flags=""
        if test -n "`echo $withval | grep ':'`"; then

