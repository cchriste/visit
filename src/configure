  "$OPERATOR_FILES" ) CONFIG_FILES="$CONFIG_FILES $OPERATOR_FILES" ;;
  "$DATABASE_FILES" ) CONFIG_FILES="$CONFIG_FILES $DATABASE_FILES" ;;
