echo $ac_n "checking for parallel features""... $ac_c" 1>&6
echo "configure:728: checking for parallel features" >&5
if test "$UseParallel" = "yes"; then
   echo "$ac_t""enabled" 1>&6
   PARALLEL="parallel"
   PARALLEL_CPPFLAGS="-DPARALLEL"
   LIBSTATICPAR="$""(LIBSTATICPAR)"
   LIBSHAREDPAR="$""(LIBSHAREDPAR)"
   LIBSHAREDPAR_UPDATEONLY="libsharedpar_updateonly"

   if test "`uname 2>/dev/null`" != "AIX"; then
       if (test -z "$MPI_LIBS") then
           MPI_LIBS="-lmpi"
       fi
   fi

   # Check if the user has specified a C compiler for MPI
   if test -n "$MPI_CC"; then
       MPI_CC="CC=$MPI_CC"
   fi

   # Check if the user has specified a C++ compiler for MPI
   if test -n "$MPI_CXX"; then
       MPI_CXX="CXX=$MPI_CXX"
   fi
else
   echo "$ac_t""disabled" 1>&6
   PARALLEL=""
   PARALLEL_CPPFLAGS=""
   MPI_LIBS=""
   SHLIB_MPI_LIBS=""
   MPI_CC=""
   MPI_CXX=""
   LIBSTATICPAR=""
   LIBSHAREDPAR=""
   LIBSHAREDTEMPPAR=""
fi









