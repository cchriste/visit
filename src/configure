  enableval="$enable_gui"
  UseGUI="$enable_gui"
else
  UseGUI=yes
fi

if test "$UseGUI" = "yes"; then
    echo "$ac_t""yes" 1>&6
    enable_gui=yes

                echo $ac_n "checking for viewer""... $ac_c" 1>&6
echo "configure:1045: checking for viewer" >&5
    if test -d viewer; then
        echo "$ac_t""found" 1>&6
        VIEWER="viewer"
        VISWINDOW="VisWindow"
        VIEWERLIB="$""(VDSO)"
        VIEWEROBJ="$""(VOBJ)"
        if test -z "$VTK"; then
