                if test "`echo $f | cut -d'L' -f1`" = "-"; then
                    deplib_dirs="$deplib_dirs `echo $f | cut -d'L' -f2-`"
                elif test "`echo $f | cut -d'l' -f1`" = "-"; then
                    deplib_libs="$deplib_libs `echo $f | cut -d'l' -f2-`"
