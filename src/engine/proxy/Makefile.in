##
## Programs and options...
##
@SET_MAKE@
@VARIABLES@

CXXFLAGS=@VTK_INCLUDES@ @CXXFLAGS@ \
         -I. -I../../include/visit

##
## Files...
##
SRC=EngineProxy.C
HDR=EngineProxy.h
OBJ=$(SRC:.C=.o)
LIB=libengineproxy
LIBS=-lenginerpc -ldbatts -lpipeline_ser -lavtexceptions -lcomm -lmisc -lstate -lplugin @VTK_LIBS@


##
## Standard targets...
##
all: $(LIBSHARED)

clean:
	$(RM) $(LIBSHARED)
	$(RM) $(OBJ)

##
## Automatic dependency stuff
##
@TARGETS@
@LIBTARGETS@
