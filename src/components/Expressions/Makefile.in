CPPFLAGS=@COMPONENT_CPPFLAGS@ @CPPFLAGS@

SEROBJ=$(SRC:.C=.o)
PAROBJ=$(SRC:.C=_par.o)
LIB=libexpressions
COMMONLIBS=-ldbatts -lavtexceptions -lstate -lcomm -lmisc -lexpr -lparser -lutility -lvisit_vtk -llightweight_visit_vtk
