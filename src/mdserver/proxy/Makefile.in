##
## Programs and options...
##
@SET_MAKE@
@VARIABLES@

CXXFLAGS=@CXXFLAGS@ -I. -I../../include/visit
CPPFLAGS=@CPPFLAGS@ -I. -I../../include/ -DMDSERVER_PROXY_EXPORTS

##
## Files...
##
SRC=MDServerProxy.C
HDR=MDServerProxy.h
OBJ=$(SRC:.C=.o)
LIB=libmdserverproxy
LIBS=-lmdserverrpc -lproxybase -ldbatts -lavtexceptions -lcomm -lmisc -lstate -lexpr -lparser -lplugin -lutility 



##
## Standard targets...
##
all: $(LIBSHARED)

clean:
	$(RM) $(LIBSHARED)
	$(RM) $(OBJ)

##
## Automatic dependency stuff
##
@TARGETS@
@LIBTARGETS@
