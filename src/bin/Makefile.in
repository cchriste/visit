##############################################################################

@SET_MAKE@
@VARIABLES@

DISTRIB= install-sh visit makemovie.py
NOT_DISTRIB= run_doxygen

all: mocprogram

mocprogram:
	if test -n "@QT_BIN@"; then               \
	    $(RM) moc                        ;   \
	    ln -s @QT_BIN@/moc moc        ;   \
	fi

clean:
	$(RM) moc *.log *.timings

distclean: clean

